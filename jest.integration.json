{
  "verbose": true,
  "collectCoverage": true,
  "testEnvironment": "node",
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "testMatch": [
    "**/__tests__/*.test.js"
  ],
  "modulePathIgnorePatterns": [
    "dist/*"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/dist/",
    "/fixtures/"
  ]
}

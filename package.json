{
  "name": "@parkhub/iris",
  "author": "Daniel Olivares",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "start": "nps",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\""
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=3"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@parkhub/eslint-config-parkhub-base": "^1.2.1",
    "@telerik/semantic-prerelease": "^1.2.1",
    "are-we-flow-yet": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "codecov": "^3.0.2",
    "commitizen": "^2.9.6",
    "condition-codefresh": "^1.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "delay": "^2.0.0",
    "documentation": "^6.3.2",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "flow-bin": "^0.71.0",
    "flow-copy-source": "^1.3.0",
    "flow-remove-types": "^1.2.3",
    "flow-typed": "^2.4.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "leasot": "^6.3.0",
    "nps": "^5.9.0",
    "nps-utils": "^1.5.0",
    "opt-cli": "^1.6.0",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.4.0",
    "validate-commit-msg": "^2.14.0"
  },
  "release": {
    "verifyConditions": "condition-codefresh",
    "debug": false
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "testMatch": [
      "**/src/**/?(*.)test.js?(x)"
    ],
    "modulePathIgnorePatterns": [
      "dist/*"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parkhub/iris.git"
  },
  "bugs": {
    "url": "https://github.com/parkhub/iris/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "avsc": "^5.2.3",
    "debug": "^3.1.0",
    "lodash.flow": "^3.5.0",
    "lodash.includes": "^4.3.0",
    "node-rdkafka": "^2.3.2",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "uuid": "^3.2.1"
  }
}
